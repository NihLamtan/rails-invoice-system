       <% @clients.each do |client| %>

                            <tr id="<%= dom_id client %>">
                                <td><%= client.name %></td>
                                <td><%= client.email %></td>
                                <td><%= client.phone %></td>
                                <td><%= client.currency %></td>
                                                          
                                <td>
                                <ul class="d-flex action-btn">
                                        <li> <%= link_to "View", client %></li>
                                        <li> <%= button_to "Delete", client, method: :delete %></li>

                                    </ul>
                                </td>

                            </tr>

      
      
       <% end %>